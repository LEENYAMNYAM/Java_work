package ch01;

public class EX1234 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str1="aaaaa";
		String str2="bbbbb";
		String answer = "";
		        
		int i=0;
		while(true){
			if(i%2==0) {
				answer+=str1.charAt(i);
		    }else{
		    	answer+=str2.charAt(i);
		    }            
		    System.out.println(answer);
		    }
	}
		
		
}

